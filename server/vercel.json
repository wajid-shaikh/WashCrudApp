{
  "version": 2,
  "functions": {
    "api": {
      "handler": "api/index.js",
      "events": [
        {
          "http": {
            "method": "GET",
            "path": "/api/hello"
          }
        }
      ]
    }
  },
  "builds": [{ "src": "*.js", "use": "@vercel/node" }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ],
  "maxDuration": 30
}
