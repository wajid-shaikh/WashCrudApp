{
  "version": 2,
  "builds": [
    {
      "src": "api/index.js",
      "use": "@vercel/node"
    },
    {
      "src": "crudByVite/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    },
    {
      "src": "^/static/(.*)",
      "dest": "/crudByVite/static/$1"
    },
    {
      "src": "/(.*)\\.png",
      "dest": "/crudByVite/$1.png"
    },
    {
      "src": "/(.*)",
      "dest": "/crudByVite/index.html"
    }
  ]
}
